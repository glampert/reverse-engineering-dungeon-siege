  fn makeMaterial =
  (
    local mm = MultiMaterial numSubs:4
    mm.name = "__GRIPMARKER_MATERIAL"
    mm.names[1] = "BLUE"
    mm.names[2] = "GREEN"
    mm.names[3] = "RED"
    mm.names[4] = "GREY"
    mm.materialList[1].name = "BLUE"
    mm.materialList[2].name = "GREEN"
    mm.materialList[3].name = "RED"
    mm.materialList[4].name = "GREY"
    mm.materialList[1].diffuse = (color 56 49 205)
    mm.materialList[2].diffuse = (color 80 229 45)
    mm.materialList[3].diffuse = (color 238 34 58)
    mm.materialList[4].diffuse = (color 49 49 49)
    local i
    for i = 1 to 4 do
    (
      mm.materialList[i].specularLevel = 5
      mm.materialList[i].glossiness = 25
    )
    return mm
  )


  on buildMesh do
  (
    local i

    if (gripType == 1) then
    (
      -- weapon_grip
      setMesh Mesh vertices:wgVerts faces:wgFaces materialIDs:wgMatIDs
      for i = 1 to wgFaces.count do
        setFaceSmoothGroup mesh i wgSmoothGroups[i]
    )
    else if (gripType == 2) then
    (
      -- shield_grip
      setMesh Mesh vertices:sgVerts faces:sgFaces materialIDs:sgMatIDs
      for i = 1 to sgFaces.count do
        setFaceSmoothGroup mesh i sgSmoothGroups[i]
    )
    else
    (
      -- root marker
      setMesh Mesh vertices:rtVerts faces:rtFaces materialIDs:rtMatIDs
      for i = 1 to rtFaces.count do
        setFaceSmoothGroup mesh i rtSmoothGroups[i]
    )
  )


  on attachedToNode n do
  (
    n.material = makeMaterial()
  )


  tool create
  (
    on mousePoint click do
      case click of
      (
        1: nodeTM.translation = gridPoint
        2: #stop
      )
  )
)